{
    "name": "app",
    "version": "0.0.17",
    "description": "A starter kit for building PWAs!",
    "main": "index.js",
    "scripts": {
        "dev-server": "vite",
        "dev": "deno run dev-server",
        "dev-task": "vite",
        "_deploy": "npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
        "tsc": "tsc",
        "build": "tsc && vite build",
        "start": "deno run dev",
        "start-remote": "vite --host"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@lit-labs/router": "^0.1.4",
        "@lit-labs/signals": "^0.1.2",
        "@shoelace-style/shoelace": "^2.20.1",
        "browser-fs-access": "^0.38.0",
        "events": "^3.3.0",
        "fhir-beacon": "0.1.4",
        "lit": "^3.3.0",
        "native-file-system-adapter": "^3.0.1",
        "signal-utils": "^0.21.1",
        "urlpattern-polyfill": "^10.1.0",
        "workbox-build": "^7.1.1",
        "workbox-core": "^7.1.0",
        "workbox-precaching": "^7.1.0"
    },
    "devDependencies": {
        "@types/node": "^24.0.3",
        "ts-lit-plugin": "^2.0.2",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-pwa": "^1.0.0"
    },
    "prettier": {
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "quoteProps": "consistent",
        "trailingComma": "es5",
        "endOfLine": "crlf",
        "bracketSpacing": true
    }
}
