{
    "name": "app",
    "version": "0.0.2",
    "description": "A starter kit for building PWAs!",
    "main": "index.js",
    "scripts": {
        "dev-server": "vite",
        "dev": "deno run dev-server",
        "dev-task": "vite",
        "_deploy": "npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
        "tsc": "tsc",
        "build": "tsc && vite build",
        "start": "deno run dev",
        "start-remote": "vite --host"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@shoelace-style/shoelace": "^2.19.0",
        "events": "^3.3.0",
        "lit": "^3.2.1",
        "@lit-labs/router": "^0.1.3",
        "@lit-labs/signals": "^0.1.1",
        "urlpattern-polyfill": "^10.0.0",
        "workbox-build": "^7.1.1",
        "workbox-core": "^7.1.0",
        "workbox-precaching": "^7.1.0",
        "fhir-beacon": "0.0.13",
        "browser-fs-access": "^0.35.0",
        "signal-utils": "^0.21.1",
        "native-file-system-adapter": "^3.0.1"
    },
    "devDependencies": {
        "ts-lit-plugin": "^2.0.2",
        "typescript": "^5.6.2",
        "vite": "^5.4.8",
        "vite-plugin-pwa": "^0.20.5"
    },
    "prettier": {
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "quoteProps": "consistent",
        "trailingComma": "es5",
        "endOfLine": "crlf",
        "bracketSpacing": true
    }
}
