{
    "name": "fhir-beacon",
    "version": "0.0.1",
    "homepage": "https://zenwork.github.io/fhir-beacon/",
    "description": "FHIR web components",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "build": "cem analyze --config build/custom-elements-manifest.config.js",
        "build-watch": "cem analyze --config build/custom-elements-manifest.config.js --watch",
        "demo": "wds --watch",
        "tsc": "tsc",
        "tsc-watch": "tsc --watch",
        "test-all-browsers": "tsc && wtr --coverage --playwright --browsers chromium firefox",
        "test": "tsc && wtr --coverage",
        "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
        "storybook": "storybook dev -p 9999",
        "storybook:clean": "rm -rf node_modules/.cache/storybook",
        "build-storybook": "storybook build",
        "publish": "npm ci && npm run build-storybook"
    },
    "private": true,
    "dependencies": {
        "@lit/context": "^1.1.1",
        "@shoelace-style/shoelace": "^2.15.1",
        "jsonpath-plus": "^9.0.0",
        "lit": "^3.1.3"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.3.3",
        "@custom-elements-manifest/analyzer": "^0.10.2",
        "@eslint/js": "^9.6.0",
        "@esm-bundle/chai": "^4.3.4",
        "@open-wc/semantic-dom-diff": "^0.20.1",
        "@open-wc/testing": "^3.0.0-next.5",
        "@playwright/test": "^1.43.1",
        "@storybook/addon-essentials": "^8.0.9",
        "@storybook/addon-links": "^8.0.9",
        "@storybook/addon-storysource": "^8.1.2",
        "@storybook/blocks": "^8.0.9",
        "@storybook/manager-api": "^8.1.5",
        "@storybook/test": "^8.0.9",
        "@storybook/theming": "^8.1.5",
        "@storybook/web-components": "^8.0.9",
        "@storybook/web-components-vite": "^8.0.9",
        "@testdeck/mocha": "^0.3.3",
        "@types/chai": "^4.3.16",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.12.8",
        "@web-types/lit": "^2.0.0-3",
        "@web/dev-server": "^0.4.5",
        "@web/dev-server-esbuild": "^1.0.2",
        "@web/dev-server-rollup": "^0.6.3",
        "@web/test-runner": "^0.18.2",
        "@web/test-runner-junit-reporter": "^0.7.1",
        "@web/test-runner-playwright": "^0.11.0",
        "@whitespace/storybook-addon-html": "^6.1.1",
        "concurrently": "^8.2.2",
        "custom-element-jet-brains-integration": "^1.4.4",
        "dotenv": "^16.4.5",
        "eslint": "^9.6.0",
        "globals": "^15.8.0",
        "lit-html": "^3.1.4",
        "mocha": "^10.4.0",
        "remark-gfm": "^4.0.0",
        "rollup-plugin-import-assert": "^3.0.1",
        "rollup-plugin-import-assertions": "^0.5.0",
        "rollup-plugin-inline-code": "^1.2.7",
        "storybook": "^8.0.9",
        "ts-lit-plugin": "^2.0.2",
        "typescript-eslint": "^8.0.0-alpha.14"
    },
    "web-types": [
        "./build/sl-web-types.json",
        "./build/web-types.json"
    ]
}
